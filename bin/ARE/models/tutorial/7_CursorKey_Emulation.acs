        <outputPort portTypeID="v">
          <properties />
        </outputPort>
        <outputPort portTypeID="pov">
          <properties />
        </outputPort>
      </ports>
      <properties>
        <property name="updatePeriod" value="100" />
      </properties>
      <layout>
        <posX>64</posX>
        <posY>140</posY>
      </layout>
    </component>
