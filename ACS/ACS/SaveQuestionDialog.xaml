<Window x:Class="Asterics.ACS.SaveQuestionDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:properties="clr-namespace:Asterics.ACS.Properties"
        Title="{x:Static properties:Resources.SaveDialogTitle}" Height="128" Width="376" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow">
    <Grid Width="354">
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="textBlock1" Text="{x:Static properties:Resources.SaveDialogText}" VerticalAlignment="Top" Width="275" />
        <Button Content="{x:Static properties:Resources.SaveButton}" Height="23" HorizontalAlignment="Left" Margin="12,41,0,0" Name="buttonSave" VerticalAlignment="Top" Width="90" IsDefault="True" Click="buttonSave_Click" />
        <Button Content="{x:Static properties:Resources.DontSaveButton}" Height="23" HorizontalAlignment="Left" Margin="122,41,0,0" Name="buttonDontSave" VerticalAlignment="Top" Width="90" Click="buttonDontSave_Click" />
        <Button Content="{x:Static properties:Resources.CancelButton}" Height="23" HorizontalAlignment="Left" Margin="232,41,0,0" Name="buttonCancel" VerticalAlignment="Top" Width="90" IsCancel="True" />
    </Grid>
</Window>
